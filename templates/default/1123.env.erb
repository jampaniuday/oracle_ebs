   #WHICH ENV:
export CHEF_ENV=11203
export _DBDIR="<%= node[:ebs][:db][:orahome3] %>"


export        HOME="<%= node[:ebs][:db][:usr][:homedir] %>"
export       LOGIN="<%= node[:ebs][:db][:usr][:name] %>"
export ORACLE_BASE="<%= node[:ebs][:db][:orabase] %>"
export ORACLE_HOME="${_DBDIR}"
export  ORACLE_SID="<%= node[:ebs][:db][:sid] %>"
export        USER="<%= node[:ebs][:db][:usr][:name] %>"

export LANG='en_US'
export LANGUAGE='en_US'
export LC_ALL='en_US'

export LD_LIBRARY_PATH="${_DBDIR}/lib:/usr/dt/lib"
export LD_LIBRARY_PATH="$LD_LIBRARY_PATH:/usr/openwin/lib:${_DBDIR}/lib"
export LD_LIBRARY_PATH="$LD_LIBRARY_PATH:${_DBDIR}/ctx/lib:"

export LIBPATH="${_DBDIR}/lib:/usr/dt/lib"
export LIBPATH="$LIBPATH:/usr/openwin/lib:${_DBDIR}/lib"
export LIBPATH="$LIBPATH:${_DBDIR}/ctx/lib:"

export LOCPATH="/usr/lib/nls/loc:/usr/vacpp/bin"

export NLSPATH="/usr/lib/nls/msg/%L/%N:/usr/lib/nls/msg/%L/%N.cat"
export NLSPATH="$NSLPATH:/usr/lib/nls/msg/%l.%c/%N:/usr/lib/nls/msg/%l.%c/%N.cat:"

export ORA_NLS10="${_DBDIR}/nls/data/9idata"

export PATH="/usr/bin:/etc:/usr/sbin"
export PATH="$PATH:/usr/ucb:/usr/bin/X11:/sbin:/usr/local/bin"
export PATH="$PATH:${_DBDIR}/bin"
export PATH="$PATH:${_DBDIR}/perl/bin"
export PATH="$PATH:${_DBDIR}/OPatch"
export PATH="$PATH:<%= node[:ebs][:cmd][:compiler] %>"
export PATH="$PATH:<%= node[:ebs][:db][:usr][:homedir] %>/bin:."

export PERL5LIB="${_DBDIR}/perl/lib"
export PERL5LIB="$PERL5LIB:${_DBDIR}/perl/lib/5.10.0"
export PERL5LIB="$PERL5LIB:${_DBDIR}/perl/lib/site_perl/5.10.0"

export SKIP_ROOTPRE='TRUE'

export SHLIB_PATH="${_DBDIR}/lib"
export SHLIB_PATH="$SHLIB_PATH:${_DBDIR}/ctx/lib"

export TNS_ADMIN="${_DBDIR}/network/admin"
export TNS_ADMIN="${TNS_ADMIN}/<%= node[:ebs][:db][:sid] %>_<%= node[:hostname] %>"


   ######################################
   #for dbms performance
   #
export LDR_CNTRL=DATAPSIZE=64K@TEXTPSIZE=64K@STACKPSIZE=64K@SHMPSIZE=64K
export AIXTHREAD_SCOPE=S


   ######################################
   # common, independent of install stage
   ######################################


alias ls='ls -FC'
alias psall="ps -aef | fgrep $USER"
alias 2h="cd \$ORACLE_HOME; pwd"
alias 2log="cd \$ORACLE_HOME/admin/${_contxt}/diag/rdbms/vis/${_sid}/trace; pwd"
alias vilog="vi \$ORACLE_HOME/admin/${_contxt}/diag/rdbms/vis/${_sid}/trace/alert_${_sid}.log"
alias stopdb="${_ebsbin}/stopdb.sh"
alias startdb="${_ebsbin}/startdb.sh"
alias asdba="sqlplus '/ as sysdba'"

export PS1='[$USER@$_$CHEF_ENV] -->${ORACLE_SID}<$PWD> '

echo
echo "\tCHANGE IN ENVIRONMENT: $CHEF_ENV"
echo "\t====== == ============ ========"
echo "\tORACLE_HOME=$ORACLE_HOME"
echo "\tORACLE_SID=$ORACLE_SID"
echo "\tSIDHNAME=$SIDHNAME"
echo "\tORA_NLS10=$ORA_NLS10"
echo "\tTNS_ADMIN=$TNS_ADMIN"

echo "\tLOGNAME=${LOGNAME}"
echo "\tLANGUAGE=$LANGUAGE"
echo "\tTERM=$TERM"
echo "\tSKIP_ROOTPRE=$SKIP_ROOTPRE"

echo
